#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/wolfy/Desktop/WORK/movies/node_modules/.pnpm/sass-graph@2.2.5/node_modules/sass-graph/bin/node_modules:/home/wolfy/Desktop/WORK/movies/node_modules/.pnpm/sass-graph@2.2.5/node_modules/sass-graph/node_modules:/home/wolfy/Desktop/WORK/movies/node_modules/.pnpm/sass-graph@2.2.5/node_modules:/home/wolfy/Desktop/WORK/movies/node_modules/.pnpm/node_modules:/home/wolfy/Desktop/WORK/movies/node_modules:/home/wolfy/Desktop/WORK/node_modules:/home/wolfy/Desktop/node_modules:/home/wolfy/node_modules:/home/node_modules:/node_modules:/home/wolfy/Desktop/WORK/movies/node_modules/.pnpm/node_modules/sass-graph/bin/node_modules:/home/wolfy/Desktop/WORK/movies/node_modules/.pnpm/node_modules/sass-graph/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/wolfy/Desktop/WORK/movies/node_modules/.pnpm/sass-graph@2.2.5/node_modules/sass-graph/bin/node_modules:/home/wolfy/Desktop/WORK/movies/node_modules/.pnpm/sass-graph@2.2.5/node_modules/sass-graph/node_modules:/home/wolfy/Desktop/WORK/movies/node_modules/.pnpm/sass-graph@2.2.5/node_modules:/home/wolfy/Desktop/WORK/movies/node_modules/.pnpm/node_modules:/home/wolfy/Desktop/WORK/movies/node_modules:/home/wolfy/Desktop/WORK/node_modules:/home/wolfy/Desktop/node_modules:/home/wolfy/node_modules:/home/node_modules:/node_modules:/home/wolfy/Desktop/WORK/movies/node_modules/.pnpm/node_modules/sass-graph/bin/node_modules:/home/wolfy/Desktop/WORK/movies/node_modules/.pnpm/node_modules/sass-graph/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sass-graph/bin/sassgraph" "$@"
else
  exec node  "$basedir/../sass-graph/bin/sassgraph" "$@"
fi
